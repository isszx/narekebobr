---
export interface Props extends astroHTML.JSX.AnchorHTMLAttributes {}

const { class: className, ...props } = Astro.props;
---

<div class:list={['part', className]} {...props}>
	<slot />
</div>
<style>
    .part {
        display: flex;
        flex-direction: column;
        margin-bottom: 30px;
    }
</style>
